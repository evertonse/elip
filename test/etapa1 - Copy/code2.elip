entrada
(inteiro main():
  (
    (fibonacci(5))
    +
    (lambda (x | y) : (se (nao (y = 0)) entao (x / y - (1 / y)) senao ((x+y) / y)) [10 | 20])
  )
)

(inteiro fibonacci (inteiro N):
  # N-esimo numero de Fibonacci .
  (se ((N = 0b0) or (N = 1)) entao
    (1) 
  senao 
    ((fibonacci (N + 1)) +(fibonacci (N + 2)))
  )
)

